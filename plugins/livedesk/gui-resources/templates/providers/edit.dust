<div class="edit-block">		<div class="top-controls">			<select name="type">				{#PostTypes}					<option value="{Key}">{Key}</option>				{/PostTypes}			</select>			<div class="toolbar-placeholder"></div>		</div>		<div class="edit-area">			<div class="url-input-holder"></div>			<article class="editable"></article>		</div>		<div class="edit-post-message" style="float:left; margin-top:-10px; width:50%"></div>				<button ci="savepost" class="btn btn-primary pull-right" style="display:none;" type="submit">_('Save &amp; post')</button>		<button ci="save" class="btn pull-right" type="submit">_('Save')</button></div><div id="own-posts-holder" class="search-result-list-own scroller" style="margin-top:7px;">    <ul id="own-posts-results" class="post-list wraped">    </ul></div>
